        "rm \"./app/webapp/$build_file_name\"",
        "echo \"Created build at development/luc/demo-builds/nokia_digital_operations/$build_file_name\""
      ]
    },
    "versions": {
      "node": 18
    },
    "dir": "demo_nokia_digiops"
